<?php
class sArray {
	private $inner;
	public function __construct(){
		$this->inner = array();
	}
	public function push($what){
		array_push($this->inner,$what);
		return this;
	}
	public function pop(){
		return array_pop($this->inner);
	}
	public function count(){
		return count($this->inner);
	}
	public function sum(){
		return array_sum($this->inner);
	}
	public function shift(){
		return array_shift($this->inner);
	}
}
